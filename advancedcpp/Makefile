.PHONY: clean All

All:
	@echo "----------Building project:[ StandardExceptions - Debug ]----------"
	@cd "StandardExceptions" && "$(MAKE)" -f  "StandardExceptions.mk"
clean:
	@echo "----------Cleaning project:[ StandardExceptions - Debug ]----------"
	@cd "StandardExceptions" && "$(MAKE)" -f  "StandardExceptions.mk" clean
