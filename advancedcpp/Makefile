.PHONY: clean All

All:
	@echo "----------Building project:[ VectorsandMemory - Debug ]----------"
	@cd "VectorsandMemory" && "$(MAKE)" -f  "VectorsandMemory.mk"
clean:
	@echo "----------Cleaning project:[ VectorsandMemory - Debug ]----------"
	@cd "VectorsandMemory" && "$(MAKE)" -f  "VectorsandMemory.mk" clean
